{
  "name": "resonance-webapp-docs",
  "version": "0.30.1",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "getgql:all": "npm run getgql:adminidentity && npm run getgql:demand && npm run getgql:storefront && npm run getgql:supply",
    "getgql:adminidentity": "get-graphql-schema https://api.adminidentity.buildresonance.com/graphql > adminidentityschema.graphql -h \"authorization=$AUTHHEADER\"",
    "getgql:demand": "get-graphql-schema https://api.demand.buildresonance.com/graphql > demandschema.graphql -h \"authorization=$AUTHHEADER\"",
    "getgql:storefront": "get-graphql-schema https://storefrontapi.buildresonance.com/graphql > storefrontschema.graphql  -h \"x-resonance-storefront-token=$TOKEN\"",
    "getgql:supply": "get-graphql-schema https://api.supply.buildresonance.com/graphql > supplyschema.graphql -h \"authorization=$AUTHHEADER\"",
    "build:gql": "node scripts/sortSchemas.js && npm run build:gql:adminidentity && npm run build:gql:demand && npm run build:gql:storefront && npm run build:gql:supply",
    "build:gql:adminidentity": "docusaurus graphql-to-doc --base adminidentity/api/graphql-admin/reference --schema ./adminidentityschema.graphql --homepage static/adminidentity/graphql-admin-schema-homepage.md",
    "build:gql:demand": "docusaurus graphql-to-doc --base demand/api/graphql-admin/reference --schema ./demandschema.graphql --homepage static/demand/graphql-admin-schema-homepage.md",
    "build:gql:storefront": "docusaurus graphql-to-doc --base demand/api/graphql-storefront/reference --schema ./storefrontschema.graphql --homepage static/demand/graphql-storefront-schema-homepage.md",
    "build:gql:supply": "docusaurus graphql-to-doc --base supply/api/graphql-admin/reference --schema ./supplyschema.graphql --homepage static/supply/graphql-admin-schema-homepage.md",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "lint": "eslint --ext .ts,.tsx",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@cmfcmf/docusaurus-search-local": "^0.11.0",
    "@docusaurus/core": "^2.3.1",
    "@docusaurus/plugin-client-redirects": "^2.3.1",
    "@docusaurus/preset-classic": "^2.3.1",
    "@graphiql/toolkit": "^0.8.0",
    "@graphql-markdown/docusaurus": "^1.16.1",
    "@graphql-tools/graphql-file-loader": "^7.5.13",
    "@mdx-js/react": "^1.6.22",
    "@svgr/webpack": "^5.5.0",
    "clsx": "^1.2.1",
    "file-loader": "^6.2.0",
    "graphiql": "^2.2.0",
    "graphql": "^16.6.0",
    "graphql-ws": "^5.11.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "redocusaurus": "^1.4.0",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.2.0",
    "@tsconfig/docusaurus": "^1.0.6",
    "@types/node": "^16.18.10",
    "@types/react": "^17.0.52",
    "@types/react-helmet": "^6.1.6",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sonarjs": "^0.17.0",
    "get-graphql-schema": "^2.1.2",
    "prettier": "^2.8.1",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
